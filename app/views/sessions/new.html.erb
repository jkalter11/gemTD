<h2>Log In</h2>

<%= form_tag session_path do %>

  <div class="form">
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %>
  </div>

  <div class="form">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>

  <%= submit_tag "Log in" %>

<% end %>
